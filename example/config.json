{
    "parameters": {
        "urls": [
            {"pattern": "/pattern/.*-(\\d+)", "names": ["storyId"]}
        ],
        "servers": {
            "local": "http://localhost:5001"
        }
    },
    "backend": [
        {
            "pattern": ".*",
            "target":"http://localhost:5001",
            "host":"localhost",
            "ttl":"10s",
            "replaceOuter": false,
            "quietFailure": false
        }
    ],
    "cache": {
        "engine": "memorycache"
    },
    "transformations":{
        "declarativeReplacements": {
            "type":"replacement",
            "query": "[cx-url]"
        }
    }
}
